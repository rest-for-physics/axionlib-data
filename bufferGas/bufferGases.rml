		<variable name="liquidHydrogenDensity" value="0.075" /> <!-- g/cm^3 -->
		<variable name="liquidNeonDensity" value="1.207" /> <!-- g/cm^3 -->
	</globals>

    <TRestAxionBufferGas name="helium" verboseLevel="warning">
        <gas name="He" density="0.0025e-6g/cm^3"/>
